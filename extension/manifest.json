{
  "manifest_version": 3,
  "name": "NEXT-CHROME-STARTER",
  "short_name": "N-C-S",
  "version": "0.0.1",
  "description": "Next.js Chrome Extension Starter, Demo Example by ibnzUK. https://github.com/ibnzUK/next-chrome-starter",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  
  "web_accessible_resources": [
    {
      "resources": ["index.html", "next/static/css/ff48674df0dc0ffc.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://*/",
    "http://*/"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["inject.js"],
       "css" : ["next/static/css/ff48674df0dc0ffc.css"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  }
}